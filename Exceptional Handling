a = 10
b = 2
try:
    print(" resource opened ")
    print(a / b)
    k = int(input(" enter a number: "))

except ZeroDivisionError as e:
    print(" error: ", e)

except ValueError as e:
    print(" error :", e)

except Exception as e:
    print(" something went wrong... ", e)

finally:
    print(" resource closed ")


